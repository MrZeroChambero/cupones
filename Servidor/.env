# Configuración de conexión MySQL
DB_DRIVER=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_NAME=bombcoupons
DB_USER=root
DB_PASSWORD=
DB_CHARSET=utf8mb4

# URL y puerto del backend (utilizado como referencia/documentación)
APP_URL=http://localhost:80/cupones/servidor
APP_PORT=80

# URL principal del frontend (para CORS)
FRONT_URL=http://localhost:5173

# Origins autorizados para CORS (separados por comas). Si se omite se usa FRONT_URL o los defaults.
ALLOWED_ORIGINS=
